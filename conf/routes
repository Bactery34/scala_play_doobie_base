
# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Users routes
GET      /api/users/list                     controllers.UserController.list()
GET      /api/users/get/:id                  controllers.UserController.get(id: ids.UserId)
GET      /api/users/getByName/:name          controllers.UserController.getByName(name: String)
POST     /api/users/create                   controllers.UserController.create
PUT      /api/users/update                   controllers.UserController.update
DELETE   /api/users/delete/:id               controllers.UserController.delete(id: ids.UserId)